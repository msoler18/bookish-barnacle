{%- liquid
  assign product = all_products[section.settings.product]
  assign product_block_classes = 'product-block mt-5 first:mt-0'
-%}

{%- if product != blank -%}
  {%- render 'product-template',
    section: section,
    product: product,
    is_feature: true
  -%}

  <script type="application/ld+json">
    {{ product | structured_data }}
  </script>
{%- else -%}
  {%- liquid
    assign media_desktop_width = section.settings.desktop_media_width | split: '_' | first
    assign content_desktop_width = section.settings.desktop_media_width | split: '_' | last
  -%}

  {%- render 'shape-defs',
    shape: section.settings.image_shape,
    shape_id: section.id,
    blob_type: 1
  -%}
  <section
    class="bg-scheme-background text-scheme-text {% if section.settings.use_gradient_background %}bg-scheme-gradient{% endif %}"
    data-color-scheme="{{ section.settings.color_scheme }}"
  >
    <div class="section-content relative z-10 mx-auto max-w-site py-section-vertical-spacing lg:px-section">
      {% if section.settings.title != '' %}
        <h2 class="inline-heading-rte font-heading heading-standard heading-style break-words px-section text-center lg:px-0 {{ settings.heading_drop_shadow_style }}">
          {{ section.settings.title }}
        </h2>
      {% endif %}

      <div class="{% if section.settings.title != '' %}section-content-top-margin{% endif %}">
        <div class="grid grid-cols-1 gap-10 lg:grid-cols-12 {{ section.settings.section_desktop_width }}">
          <div class="product-media col-span-full {{ media_desktop_width }} px-section lg:px-0">
            <div>
              {%- render 'media-image-placeholder',
                shape_id: section.id,
                container_id: section.id,
                shape: section.settings.image_shape,
                crop: section.settings.crop,
                is_card: false
              -%}
            </div>
          </div>
          <div class="product-content col-span-full {{ content_desktop_width }} px-section lg:flex lg:items-center lg:px-0">
            <div class="product-block-container w-full max-w-full xl:grid xl:grid-cols-12">
              <div class="relative xl:col-span-10 xl:col-start-2">
                <div class="{{ product_block_classes }} product-block-title">
                  <h2 class="font-heading heading-feature break-words">
                    {{ 'homepage.onboarding.product_title' | t }}
                  </h2>
                </div>
                {%- liquid
                  if settings.desktop_feature_heading_scale > 4
                    assign price_text_size = 'text-base lg:text-lg'
                  else
                    assign price_text_size = 'text-base'
                  endif
                -%}
                <div class="{{ product_block_classes }} product-block-price">
                  <span
                    class="text-scheme-text {{ price_text_size }}"
                  > asd asd asd asd asd asd a d
                    {% if settings.currency_code_enabled == true -%}
                      {{- 1999 | money_with_currency -}}
                    {%- else -%}
                      {{- 1999 | money -}}
                    {%- endif %}
                  </span>
                </div>
                <div class="{{ product_block_classes }}">
                  <button
                    class="add-to-cart-btn  push-btn mb-1 w-full"
                  >
                    <span class="push-btn__surface w-full"> asd
                      {{- 'products.product.add_to_cart' | t -}}
                    </span>
                  </button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    {%- liquid
      for block in section.blocks
        case block.type
          when 'background_shape'
            render 'block-background-shape', block: block
          when 'background_pattern'
            render 'block-background-pattern', block: block, scheme: section.settings.color_scheme
        endcase
      endfor
    -%}
  </section>

  {%- liquid
    assign stickers = section.blocks | where: 'type', 'sticker'
    assign shape_dividers = section.blocks | where: 'type', 'shape_divider'
    for sticker in stickers
      render 'block-sticker', block: sticker
    endfor
    for shape_divider in shape_dividers
      render 'block-shape-divider', block: shape_divider
    endfor
  -%}
{%- endif -%}
