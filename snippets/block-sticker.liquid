          {%- else -%}
            {%- if block.settings.shape == 'fit_to_text' -%}
              <div class="sticker__fit-to-text {{ block.settings.color }} {{ settings.sticker_font }} whitespace-nowrap">
                {{ block.settings.text }}
              </div>
            {%- else -%}
              <div class="sticker__container rotate-anim relative">
                <div
                  class="absolute inset-0 {{ block.settings.color }}"
                  style="clip-path: url(#clip-{{ block.id }})"
                >
                  <div
                    id="sticker-text-{{ block.id }}"
                    class="sticker__text absolute inset-0 flex items-center justify-center text-center {{ settings.sticker_font }}"
                  >
                    <div>{{ block.settings.text | newline_to_br }}</div>
                  </div>
                </div>
                <svg
                  class="sticker__width-container relative max-w-none {{ section.settings.color | split: ' ' | last }}"
                  viewBox="0 0 200 200"
                  xmlns="http://www.w3.org/2000/svg"
                  xmlns:xlink="http://www.w3.org/1999/xlink"
                >
                  <use class="sticker__svg fill-transparent" href="#outline-{{ block.id }}" />
                </svg>
                {%- if settings.sticker_drop_shadow_size > 0 -%}
                  {%- if settings.sticker_shadow_color
                      == 'color-scheme-gradient'
                  -%}
                    <div class="sticker__svg sticker__drop-shadow">
                      <div
                        class="aspect-h-1 aspect-w-1 absolute h-full w-full bg-scheme-gradient"
                        style="clip-path: url(#clip-{{ block.id }})"
                      ></div>
                    </div>
                    <svg
                      class="sticker__svg sticker__drop-shadow"
                      viewBox="0 0 200 200"
                      xmlns="http://www.w3.org/2000/svg"
                      xmlns:xlink="http://www.w3.org/1999/xlink"
                    >
                      {% if settings.sticker_show_border_on_shadow %}
                      <use class="sticker__drop-shadow-border-only"  href="#outline-{{ block.id }}" />
                      {% endif %}
                    </svg>
                  {%- else -%}
                    <svg
                      class="sticker__svg sticker__drop-shadow"
                      viewBox="0 0 200 200"
                      xmlns="http://www.w3.org/2000/svg"
                      xmlns:xlink="http://www.w3.org/1999/xlink"
                    >
                      <use class="sticker__drop-shadow-path"  href="#outline-{{ block.id }}" />
                    </svg>
                  {%- endif -%}
                {%- endif -%}
              </div>
            {%- endif -%}
          {%- endif -%}
        </div>
      </div>
    </div>
  </div>
</div>

<style>
  {% if block.settings.half_width_on_mobile %}
    #sticker-{{ block.id }} {
      --block-width: {{ block_width | divided_by: 2 }};
    }

    @media (min-width: 990px) {
      #sticker-{{ block.id }} {
        --block-width: {{ block_width }};
      }
    }
  {% else %}
    #sticker-{{ block.id }} {
      --block-width: {{ block_width }};
    }
  {% endif %}

  #sticker-{{ block.id }} {
    top: {{ block.settings.vertical_position }}%;
    left: {{ block.settings.horizontal_position }}%;
  }

  #sticker-{{ block.id }} .sticker__translate-container {
    transform: translate(-50%, -50%);
  }

  #sticker-{{ block.id }} .sticker__rotate-container {
    transform: rotate({{ block.settings.rotate }}deg);
  }

  #sticker-text-{{ block.id }},
  #sticker-{{ block.id }} .sticker__fit-to-text  {
    --adjusted-sticker-font-size: calc({{ block.settings.sticker_font_size }} * 1.5);
    font-size: calc(var(--adjusted-sticker-font-size) * var(--block-width) / 300);
    line-height: 1;
  }

  #sticker-text-{{ block.id }} {
    padding: calc(2rem * var(--block-width) / 300);
  }

  #sticker-{{ block.id }} .sticker__width-container {
    width: calc(var(--block-width) * 1px);
  }
</style>

{% unless request.design_mode %}
  {% if block.settings.enable_parallax
    or block.settings.rotation_animation == 'rotate_when_scrolling'
  %}
    <script src="{{ 'shapes-animations.js' | asset_url }}" defer></script>
  {% endif %}
{%- endunless -%}
